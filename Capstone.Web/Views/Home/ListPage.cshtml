@model List<Capstone.Web.Models.BreweryModel>

@{
    ViewBag.Title = "ListPage";

}

<div class="container">
    
    
    <div class="searchList">       
       @foreach (var brewery in Model)
       {
          <p>@brewery.BreweryName</p>
          @Html.ActionLink($"{brewery.BreweryName}", "BreweryInfo", "Home", brewery.BreweryId)
         )
       }

    </div>
</div>


