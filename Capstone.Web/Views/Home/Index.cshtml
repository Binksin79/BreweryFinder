@{
    ViewBag.Title = "Home Page";
    @model Capstone.Web.Models.SearchResultModel
}

<div class="container">
    <div class="ageCheckerDiv">
        <input class="ageCheckerForm" type="date" name="ageCheckerForm"/>
        <button id="ageSubmit">Submit</button>
    </div>
    <div class="searchBarDiv">
        <form method="get" action="@Url.Action("ListPage","Home")">                    
            <input style="text-align: center" class="searchBar" type="search" name="" placeholder="Search for beers or breweries" />
        </form>
    </div>
</div>